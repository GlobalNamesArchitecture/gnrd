	<script type="text/javascript">
		function setStat(name, value, threshold) {
			if (value > threshold){
				$("#" + name).css("color", "red");
			}
			$("#" + name).html(value + "ms");
		}
		if (typeof performance !== "undefined"){
			setStat("statsDomLoad", new Date().getTime() - performance.timing.navigationStart, 500);
		}

		window.onload = function(){
			setTimeout(function(){
				if (typeof performance !== "undefined"){
				    var t = performance.timing;
					setStat("statsLoad", t.loadEventEnd - t.navigationStart, 1000);
				}
			}, 0);
		}
	</script>

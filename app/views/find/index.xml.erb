<?xml version="1.0" encoding="UTF-8"?>
<result>
<total><%= @output[:total] -%></total>
<status><%= @output[:status] -%></status>
<execution_time>
  <find_names_duration><%= @output[:execution_time][:find_names_duration] -%></find_names_duration>
  <total_duration><%= @output[:execution_time][:total_duration] -%></total_duration>
</execution_time>
<engines>
<% @output[:engines].each do |engine| -%>
  <engine><%= engine -%></engine>
<% end %>
</engines>
<names xmlns:dwc="http://rs.tdwg.org/dwc/terms/">
<% @output[:names].each do |name| -%>
  <name>
    <verbatim><%= name[:verbatim] -%></verbatim>
    <dwc:scientificName><%= name[:scientificName] -%></dwc:scientificName>
    <%- if name[:offsetStart] && name[:offsetEnd] -%>
    <offsets>
      <offset start="<%= name[:offsetStart] %>" end="<%= name[:offsetEnd] %>" />
    </offsets>
    <%- end -%>
  </name>
<% end -%>
</names>
</result>